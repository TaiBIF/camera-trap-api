{
  "name": "multimedia-annotations",
  "base": "MultimediaQueryShared",
  "http": {"path": "/media/annotation"},
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "BulkNormalize": true,
    "BulkWrite": true,
    "CustomQuery": true,
    "AccessControl": true
  },
  "properties": {
    "type": {
      "type": "string",
      "default": "StillImage"
    },
    "url": {
      "type": "string",
      "required": true
    },
    "url_md5": {
      "type": "string",
      "id": true,
      "required": true
    },
    "multimedia_error_flag": {
      "type": "boolean",
      "default": false
    },
    "related_upload_sessions": {
      "type": [
        "string"
      ],
      "default": []
    },
    "tokens": {
      "type": [
        "object"
      ],
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "multimediaTokens": {
      "type": "embedsMany",
      "model": "Tokens",
      "property": "tokens",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
