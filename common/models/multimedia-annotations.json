{
  "name": "MultimediaAnnotations",
  "base": "CameraTrapBase",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "type": {
      "type": "string",
      "default": "StillImage"
    },
    "url": {
      "type": "string",
      "required": true
    },
    "url_md5": {
      "type": "string",
      "required": true
    },
    "multimedia_error_flag": {
      "type": "boolean",
      "default": false
    },
    "related_upload_session": {
      "type": [
        "string"
      ]
    },
    "tokens": {
      "type": [
        "object"
      ],
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "multimediaTokens": {
      "type": "embedsMany",
      "model": "Tokens",
      "property": "tokens",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
