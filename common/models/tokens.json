{
  "name": "Tokens",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "token_id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuid"
    },
    "token_error_flag": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "species_shortcut": {
      "type": "string"
    },
    "description_level": {
      "type": "string",
      "required": true,
      "default": "individual"
    },
    "bbox": {
      "type": "object",
      "required": true
    },
    "meta": {
      "type": "object",
      "required": true
    }
  },
  "relations": {
    "tokenBoundingBox": {
      "type": "embedsOne",
      "model": "TokenBoundingBox",
      "property": "bbox",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    },
    "tokenData": {
      "type": "embedsMany",
      "model": "TokenData",
      "property": "data",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }

    }
  }
}
