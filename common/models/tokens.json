{
  "name": "Tokens",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "token_id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuid"
    },
    "token_error_flag": {
      "type": "boolean",
      "default": false
    },
    "species_shortcut": {
      "type": "string",
      "default": "尚未辨識"
    },
    "description_level": {
      "type": "string",
      "default": "individual"
    },
    "bbox": {
      "type": "object",
      "required": true
    },
    "meta": {
      "type": "object",
      "required": true
    },
    "data": {
      "type": [
        "object"
      ],
      "required": true
    },
    "video_paused_at": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "tokenBoundingBox": {
      "type": "embedsOne",
      "model": "TokenBoundingBox",
      "property": "bbox",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }
    },
    "tokenData": {
      "type": "embedsMany",
      "model": "TokenData",
      "property": "data",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }
    },
    "tokenMeta": {
      "type": "embedsOne",
      "model": "TokenMeta",
      "property": "meta",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
