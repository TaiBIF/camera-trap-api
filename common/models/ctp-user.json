{
  "name": "CtpUser",
  "base": "PersistedModel",
  "http": {"path": "/ctp-user"},
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "BulkWrite": true,
    "BulkNormalize": true,
    "CustomQuery": true,
    "TimeStamp": true
  },
  "properties": {
    "user_id": {
      "type": "string",
      "id": true,
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "project_roles": {
      "type": [
        "object"
      ]
    }
  },
  "validations": [],
  "relations": {
    "userProjectRoles": {
      "type": "embedsMany",
      "model": "UserProjectRole",
      "property": "project_roles",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
