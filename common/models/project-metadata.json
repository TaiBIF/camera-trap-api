{
  "name": "project-metadata",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "BulkWrite": true,
    "BulkNormalize": true,
    "CustomQuery": true,
    "AccessControl": true,
    "TimeStamp": true
  },
  "properties": {
    "project": {
      "type": "string",
      "required": true,
      "id": true
    },
    "short": {
      "type": "string",
      "required": true
    },
    "contracting_organization": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true
    },
    "pi": {
      "type": "string",
      "required": true
    },
    "from_date": {
      "type": "string",
      "required": true
    },
    "to_date": {
      "type": "string",
      "required": true
    },
    "data_from_ts": {
      "type": "number"
    },
    "data_to_ts": {
      "type": "number"
    },
    "admin_area": {
      "type": [
        "string"
      ]
    },
    "abstract": {
      "type": "string"
    },
    "remarks": {
      "type": "string"
    },
    "cover_image": {
      "type": "string"
    },
    "license": {
      "type": "object"
    },
    "data_field_enabled": {
      "type": [
        "string"
      ]
    },
    "locations": {
      "type": [
        "object"
      ]
    },
    "daily_test_time": {
      "type": [
        {
          "since": "number",
          "time": "string"
        }
      ]
    }
  },
  "validations": [],
  "relations": {
    "projectLocations": {
      "type": "embedsMany",
      "model": "LocationMetadata",
      "property": "locations",
      "options": {
        "validate": true,
        "forceId": false,
        "persistent": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
